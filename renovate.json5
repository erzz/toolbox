{
  $schema: "https://docs.renovatebot.com/renovate-schema.json",
  extends: [
    "config:recommended",
    ":semanticCommits",
    ":semanticCommitTypeAll(chore)",
    ":automergePatch",
    "group:docusaurusMonorepo",
    "group:linters",
  ],
  branchPrefix: "rv8-",
  ignorePaths: [".github/workflows/deprecated/"],
  prConcurrentLimit: 10,
  postUpdateOptions: ["yarnDedupeHighest"],
}
